[
  {
    "additionalText": "Буря мглою небо кроет,",
    "additionalText0": "Вихри снежные крутя",
    "additionalText1": "То, как зверь, она завоет",
    "additionalText2": "То заплачет, как дитя",
    "additionalText3": "То по кровле обветшалой",
    "additionalText4": "То, как путник запоздалый,",
    "additionalText5": "К нам в окошко застучит.",
    "additionalText6": "To be contninued"
  }
]
